<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".phase7.activities.CountriesListActivity">

    <androidx.fragment.app.FragmentContainerView
        android:id="@+id/country_setting_fragment_container_view"
        android:name="com.example.talkademy_phase4.phase7.fragments.CountryFragment"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0"
        tools:layout="@layout/fragment_country" />

    <ProgressBar
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/country_setting_fragment_container_view"
        app:layout_constraintEnd_toEndOf="@+id/country_setting_fragment_container_view"
        app:layout_constraintStart_toStartOf="@+id/country_setting_fragment_container_view"
        app:layout_constraintTop_toTopOf="@+id/country_setting_fragment_container_view" />



</androidx.constraintlayout.widget.ConstraintLayout>